<%- include('header') %>
  <div class="container">
        <div class="row">
            <h1>Password Category Lists</h1>
            <br>
            <form action="/passwordCategory/search" method="post">
              <input type="text" placeholder="Search Here" name="searchText" id="st">
              <input type="submit" value="Submit">
            </form>
         <table class="table table-striped table-primary">
          <thead>
            <tr>
              <th>Password Category Name</th>
        
              <th>Action</th>
            </tr>
          </thead>
          <tbody>
           <% if(records.length >0){
            records.forEach(function(row){
            %>
            <tr>
              <td><%= row.passord_category %></td>
             
              <td><a class="btn btn-primary btn-xs" href="/passwordCategory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
                <a class="btn btn-danger btn-xs" href="/passwordCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
              </td>
            </tr>
            <%  }) }
            else{
              %>
              <tr>
                <td>No Data Available</td>
               
              </tr>
              <%
            }%>
          </tbody>
        </table>
      </div>
    </div>
    <%- include('footer') %>